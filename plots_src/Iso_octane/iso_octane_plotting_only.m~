%Iso_air regime diagram plotting only

load('sw_detonation/plots_src/Iso_octane/iso_air_limits_shockwaves.mat')


% Ploting limits
figure
hold on
plot(omegas_RRE(2:end-1),chis_RRE(2:end-1),'k', 'LineWidth',2,'MarkerSize',10) %ploting RRE<->... limit
plot(omegas_RRR_BPR,chis_RRR_BPR,'b','LineWidth',2,'MarkerSize',10) %ploting RRR<->BPR limit Yann's method
plot(omegas_BPR_FNR,chis_BPR_FNR,'r','LineWidth',2,'MarkerSize',10) %ploting BPR<->FNR lim
plot(omegas_FPR_TNR,chis_FPR_TNR,'g', 'LineWidth',2,'MarkerSize',10) %ploting FPR<->TNR lim Yann's method
plot(omegas_TNR_LSR(2:end),chis_TNR_LSR(2:end),'m', 'LineWidth',2,'MarkerSize',10)  %ploting TNR<->LSR lim


% Points of interest for curved-planar case
plot(cp_rre_ang,cp_rre_khi,'*k','LineWidth',2,'MarkerSize',10);
plot(cp_rrr_ang,cp_rrr_khi,'*b','LineWidth',2,'MarkerSize',10);
plot(cp_bpr_ang,cp_bpr_khi,'*r','LineWidth',2,'MarkerSize',10);
plot(cp_fpr_ang,cp_fpr_khi,'*g','LineWidth',2,'MarkerSize',10);
plot(cp_tnr_ang,cp_tnr_khi,'*m','LineWidth',2,'MarkerSize',10);

cp_lgd = {"RRE CFD sims for planar shock and curved interface", ...
    "RRR CFD sims for planar shock and curved interface",...
    "BPR CFD sims for planar shock and curved interface",...
    "FPR CFD sims for planar shock and curved interface",... 
    "TNR CFD sims for planar shock and curved interface"};

% Points of interest for curved-curved case
plot(cc_rre_ang,cc_rre_khi,'+k','LineWidth',2,'MarkerSize',10);
plot(cc_rrr_ang,cc_rrr_khi,'+b','LineWidth',2,'MarkerSize',10);
plot(cc_bpr_ang,cc_bpr_khi,'+r','LineWidth',2,'MarkerSize',10);
plot(cc_fpr_ang,cc_fpr_khi,'+g','LineWidth',2,'MarkerSize',10);
plot(cc_tnr_ang,cc_tnr_khi,'+m','LineWidth',2,'MarkerSize',10);

cc_lgd = {"RRE CFD sims for curved shock and curved interface", ...
    "RRR CFD sims for curved shock and curved interface",...
    "BPR CFD sims for curved shock and curved interface",...
    "FPR CFD sims for curved shock and curved interface",... 
    "TNR CFD sims for curved shock and curved interface"};

legends=legend("RRE->... (graphical resolution)",...
    "RRR<->BPR","BPR<->FNR","FPR<->TNR","TNR<->LSR",...
    "RRE CFD sims for planar shock and curved interface", ...
    "RRR CFD sims for planar shock and curved interface",...
    "BPR CFD sims for planar shock and curved interface",...
    "FPR CFD sims for planar shock and curved interface",... 
    "TNR CFD sims for planar shock and curved interface",...
    "RRE CFD sims for curved shock and curved interface", ...
    "RRR CFD sims for curved shock and curved interface",...
    "BPR CFD sims for curved shock and curved interface",...
    "FPR CFD sims for curved shock and curved interface",... 
    "TNR CFD sims for curved shock and curved interface",);

legends.Location = 'NorthEast';
set(gca,'FontSize',16,'FontWeight','bold','LineWidth',1.5)
xlabel("\omega_i (deg)",'FontSize',20, 'FontWeight', 'bold')
ylabel("\chi", 'FontSize', 20, 'FontWeight', 'bold')
xlim([10,90])
ylim([0,1])
hold off